print('列式','ax+by=c\n     dx+ey=f')
a=int(input('a=?'))
b=int(input('b=?'))
c=int(input('c=?'))
d=int(input('d=?'))
e=int(input('e=?'))
f=int(input('f=?'))
if (a*e-d*b)==0:
  print('too many')
  print(exit) 
  exit() 
else:
  x=str(round((c*e-f*b)/(a*e-d*b),2))
  y=str(round((a*f-d*c)/(a*e-d*b),2))
  if (x==0) or (y==0):
    print(exit) 
    exit() 
    ('no answer')
  else:
    print('%-2d,%-02d' %(x,y))
